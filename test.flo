import System;

module Program {
  let Main(args:array[string]) = {
    const n:int = 100;
    var primes:array[bool] = array[bool](n + 1);

    for i in (2 -> n) {
      primes[i] = true;
    }
    for i in (2 -> n) where i * i < n {
      if primes[i] is true {
        var j = i * i;
        while (j < n) {
          primes[j] = false;
          j += i;
        }
      }
    }
    for i in (2 -> n) where primes[i] {
      Print(i.string);
    }
  }
}

